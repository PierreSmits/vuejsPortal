<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>Example API Services</description>
    <vendor></vendor>
    <version>1.0</version>
    
    <!-- Pour l'instant non utilisÃ© -->
    
    <service name="getOrderToPrepare" engine="java" location="com.otoit.spapi.SPAPISServices" invoke="getOrderToPrepare">
      <implements service="apisService"/>
      <attribute name="orders" mode="OUT" type="List"/>
    </service>

    <service name="startOrderPreparation" engine="java" location="com.otoit.spapi.SPAPISServices" invoke="startOrderPreparation">
      <implements service="apisService"/>
      <attribute name="orderId" mode="IN" type="String"/>
      <attribute name="shipGroup" mode="IN" type="String"/>
      <attribute name="newStatus" mode="OUT" type="String"/>
    </service>


</services>
